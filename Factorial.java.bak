import java.util.*;
class Factorial{
	public static void main(String args[]){
		int n,f=1;
		Scanner sc=new Scanner(System.in);
		System.out.println("enter the number:");
		n=sc.nextInt();
		for(int i=0;i<n;i++){
			f=f*i;
			
		}
		System.out.println("the factorial of the number is"+f);
	}
}