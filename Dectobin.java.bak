import java.util.*;
import java.io.*;
class Dectobin{
	public static void main(String args[]){
		int n;
		Scanner sc=new Scanner(System.in);
		System.out.println("enter the number:");
		n=sc.nextInt();
		int[] binary=new int[10];
		int index=0;
		while(n>0){
			binary[index++]=n%2;
			n=n/2;
			
		}
		for(int i=0;i<index-1;i++){
			System.out.print(binary[i]+" ");
		}
		System.out.print(" ");
	}
}